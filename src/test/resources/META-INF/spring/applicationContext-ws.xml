<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

		<bean id="plantService"
			class="org.springframework.remoting.jaxws.JaxWsPortProxyFactoryBean">
			<property name="serviceName" value="PlantSOAPServiceService" />
			<property name="serviceInterface"
				value="cs.ut.domain.soap.client.PlantSOAPService" />
			<property name="wsdlDocumentUrl"
				value="${plantSOAPService.wsdlDocumentUrl}" />
			<property name="namespaceUri" value="http://service.soap.domain.ut.cs/" />
			<property name="portName" value="PlantSOAPServicePort" />
		</bean>
		
		<bean id="purchaseOrderSOAPService"
			class="org.springframework.remoting.jaxws.JaxWsPortProxyFactoryBean">
			<property name="serviceName" value="PurchaseOrderSOAPServiceService" />
			<property name="serviceInterface"
				value="cs.ut.domain.soap.client.po.PurchaseOrderSOAPService" />
			<property name="wsdlDocumentUrl"
				value="${purchaseOrderSOAPService.wsdlDocumentUrl}"  />
			<property name="namespaceUri" value="http://service.soap.domain.ut.cs/" />
			<property name="portName" value="PurchaseOrderSOAPServicePort" />
		</bean>
</beans>